<body class="gray-bg">
<div id="list" class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-xs-24 col-sm-24 col-md-24 col-lg-24">
            <div class="ibox float-e-margins">
                <div class="ibox-content">
                    <form id="searchForm" class="form-horizontal">
                        <div class="form-group form-group-sm">
                            <div class="input-tip">
                                <label class="col-xs-3 col-sm-3 col-md-3 col-lg-3 control-label">记录时间</label>
                                <div class="col-xs-5 col-sm-5 col-md-5 col-lg-5">
                                    <input class="form-control" name="jcsj"/>
                                </div>
                            </div>
                            <div class="input-tip">
                                <label class="col-xs-3 col-sm-3 col-md-3 col-lg-3 control-label">记录类型</label>
                                <div class="col-xs-5 col-sm-5 col-md-5 col-lg-5">
                                    <select name="lx" class="form-control">
                                        <option value="">--请选择--</option>
                                        <option value="0">货运车站</option>
                                        <option value="1">客运车站</option>
                                        <option value="2">列车</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-xs-24 col-sm-24 col-md-24 col-lg-24 text-right clearfix">
                                <button type="button" class="btn btn-primary" id="btn-search" modal="enter"><span
                                        class="glyphicon glyphicon-search"></span><span>&nbsp;查询&nbsp;</span>
                                </button>&nbsp;&nbsp;
                                <button type="button" class="btn btn-white" modal="reset"><span
                                        class="glyphicon glyphicon-remove"></span><span>&nbsp;清除查询条件&nbsp;</span>
                                </button>
                                <button type="button" class="btn btn-primary hide" id="exportBtn"
                                        style="position: absolute; right: 0; top: -6px;"/>
                                <span>&nbsp;导出&nbsp;</span>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
            <div class="ibox float-e-margins">
                <div class="ibox-content">

                    <div id="todo" class="tab-pane active">
                        <div style="text-align:right;">
                            <input type="button" id="push" class="btn btn-primary " value="推送问题"/>
                            <input type="button" id="add" class="btn btn-primary " value="生成记录"/>
                        </div>
                        <table id="table_gzjlsc" data-toggle="table" data-ajax="table_gzjlsc" class="table">
                            <thead>
                            <tr>
                                <th data-field="state" data-checkbox="true">选择</th>
                                <th data-field="id">序号</th>
                                <th data-field="rwmc">任务名称</th>
                                <th data-field="gzjlbh">记录编号</th>
                                <th data-field="jllx" data-formatter="lx_formatter">记录类型</th>
                                <th data-field="jlsj">记录时间</th>
                                <th data-field="jlxz">记录小组</th>
                                <th data-field="jcxq" data-switchable="false" data-formatter="handle_1"
                                    data-events="tableEvent_1">操作
                                </th>
                            </tr>
                            </thead>
                        </table>

                    </div>

                </div>
            </div>
        </div>
    </div>
</div>
</body>
