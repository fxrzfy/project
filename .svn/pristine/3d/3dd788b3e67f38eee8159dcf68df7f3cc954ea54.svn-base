    <div id="Dialog_modify_password" class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel"
        data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-body">
                    <div class="page-header">
                        <h3 class="text-center">修改系统密码</h3>
                    </div>
                    <form class="form-horizontal" id="modifyPWD" role="form">
                        <input type="hidden" name="uid">
                        <div class="form-group">
                            <label for="oldpass" class="col-xs-10 col-sm-10 col-md-10 control-label">原密码</label>
                            <div class="col-xs-14 col-sm-14 col-md-14">
                                <input type="password" name="oriPassword" class="form-control required" style="width:250px;"
                                    id="oldpass" placeholder="请输入原密码" oninput="ExistsData()" onpaste="return false;">
                                </span>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="newpass" class="col-xs-10 col-sm-10 col-md-10 control-label">新密码</label>
                            <div class="col-xs-14 col-sm-14 col-md-14">
                                <input type="password" name="password" id="password" class="form-control required"
                                    style="width:250px;" id="newpass" placeholder="请输入新密码" oninput="ExistsData()"
                                    onpaste="return false;">
                                </span>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="newpassAgain" class="col-xs-10 col-sm-10 col-md-10 control-label">再次确认新密码</label>
                            <div class="col-xs-14 col-sm-14 col-md-14">
                                <input type="password" name="password_again" class="form-control required" style="width:250px;"
                                    id="newpassAgain" placeholder="请再次输入新密码" oninput="ExistsData()" onpaste="return false;">
                                </span>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-xs-10 col-sm-10 col-md-10 m-l control-label"> </label>
                            <button type="button" class="btn btn-primary" id="modifyPwdSubmit" style="text-align:center;"
                                disabled="disabled">
                                确认修改
                            </button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
